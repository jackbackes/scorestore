{
    "plugins": ["plugins/markdown", "plugins/summarize"],
    "opts": {
      "recurse": true,
      "destination": "./docs"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["./browser", "./server", "./tests"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    }
}
