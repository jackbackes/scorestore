{
    "plugins": ["plugins/markdown", "plugins/summarize"],
    "opts": {
      "recurse": true
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["./browser", "./server", "./tests"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    },
    "destination": "./out"
}
